<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.google.mlkit.samples.nl.languageid"
    tools:overrideLibrary="com.google.android.gms.base">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!-- Optional: Add it to automatically download ML model to device after
      your app is installed.-->
    <meta-data
        android:name="com.google.mlkit.vision.DEPENDENCIES"
        android:value="langid"/>

    <activity
            android:name="com.google.mlkit.samples.languageid.java.MainActivity"
            android:theme="@style/MaterialAppTheme" />
        <activity
            android:name="com.google.mlkit.samples.languageid.kotlin.MainActivity"
            android:theme="@style/MaterialAppTheme" />
        <activity
            android:name="com.google.mlkit.samples.languageid.EntryChoiceActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/MaterialAppTheme" />
    </application>

</manifest>
